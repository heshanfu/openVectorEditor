{
  "name": "open-vector-editor",
  "version": "0.2.0",
  "description": "Teselagen's Open Source Vector Editor",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": {
    "name": "tnrich",
    "email": "tnrich@teselagen.com",
    "url": "https://github.com/tnrich"
  },
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3344",
    "precommit": "lint-staged",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "test": "jest --env=jsdom --maxWorkers 2",
    "test-coverage": "yarn test -- --coverage",
    "test:staged": "cross-env CI=true yarn test -- --findRelatedTests",
    "test-fix": "git reset . && cross-env CI=true yarn test --onlyChanged --updateSnapshot",
    "test-watch": "yarn test --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "@blueprintjs/labs": "^0.12.0",
    "apollo-client": "^2.0.0",
    "basiccontext": "^3.5.1",
    "bio-parsers": "^4.2.2",
    "classnames": "^2.2.5",
    "clipboard": "^1.7.1",
    "clone": "^2.1.1",
    "color": "^2.0.0",
    "combokeys": "^3.0.0",
    "cross-env": "^5.1.0",
    "deep-equal": "^1.0.1",
    "file-saver": "^1.3.3",
    "graphql": "^0.11.7",
    "hint.css": "^2.5.0",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.4",
    "lodash.flatmap": "^4.5.0",
    "lru-memoize": "^1.0.2",
    "material-ui": "^0.19.4",
    "math-expression-evaluator": "^1.2.17",
    "nodemon": "^1.12.1",
    "pascal-case": "^2.0.1",
    "paths-js": "^0.4.5",
    "pluralize": "^7.0.0",
    "query-string": "^5.0.1",
    "randomcolor": "^0.5.3",
    "rc-slider": "^8.3.3",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-container-dimensions": "^1.3.2",
    "react-dimensions": "^1.3.0",
    "react-draggable": "^3.0.3",
    "react-dropzone": "^4.2.1",
    "react-icons": "^2.2.7",
    "react-list": "^0.8.8",
    "react-mouse-aware": "^0.3.0",
    "react-popover": "^0.5.4",
    "react-sanfona": "^1.0.0-rc3",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-act": "^1.4.1",
    "reselect": "^3.0.1",
    "teselagen-interval-tree": "1.1.1",
    "teselagen-react-components": "^18.1.8",
    "tether": "^1.4.0",
    "uniqid": "^4.1.1",
    "uuid": "^3.1.0",
    "ve-range-utils": "^2.0.44",
    "ve-sequence-utils": "3.1.31"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^1.18.0",
    "@blueprintjs/datetime": "^1.16.0",
    "@blueprintjs/table": "^1.15.0",
    "apollo-cache-invalidation": "^0.0.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-apollo": "^2.0.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.6.0",
    "tg-client-query-builder": "^2.12.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.32.0",
    "@blueprintjs/datetime": "^1.23.0",
    "@blueprintjs/table": "^1.29.1",
    "apollo-cache-invalidation": "^0.0.3",
    "axios": "^0.17.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chance": "^1.0.11",
    "enzyme": "^3.1.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-teselagen": "^4.0.8",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "install-peerdeps": "^1.2.0",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "nwb": "^0.19.0",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-perf": "^15.4.2",
    "react-apollo": "1.4.15",
    "react-collapsible": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10",
    "react-table": "^6.6.0",
    "redux-form": "^7.1.1",
    "redux-thunk": "*",
    "tg-client-query-builder": "^2.12.1"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/openVectorEditor",
  "keywords": [
    "react-component",
    "plasmid",
    "vector",
    "editor",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
